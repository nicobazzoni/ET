<div class = "alien_faces">
  <%= image_tag("logos/abstract-planets.jpeg") %>
 </div>


<ul class="no-bullets">
<div class = "card">
    <h1> Create New Planet </h1>
<%= link_to " 🌍 ", new_planet_path %><br>
<h1> Planets</h1>
<% @planets.each do |planet| %>
