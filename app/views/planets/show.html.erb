<%= link_to @planet.name, @planet %><br>





<% @planet.alien_planets.each do |alien_planets| %> <br>
  <% planet_species = @planets.species.find(alien_planets.species_id) %> <br>
  <%= link_to @planet_species.name, planet_path(@planet_species) %> <br>
  <%= link_to alien_species.name, alien_species_path(alien_species) %> <br>
<% end %>